{
    "version": 2,
    "name": "music-album",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/images/(.*)", "dest": "/images/$1" }
 
    ],
    "static": {
      "source": "images",
      "destination": "/images"
    }
  }
  